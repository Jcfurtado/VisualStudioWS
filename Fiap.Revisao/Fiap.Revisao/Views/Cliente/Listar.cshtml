@model List<Fiap.Revisao.Models.Cliente>
@{
    ViewBag.Title = "Listar";
}

<h2>Lista de Clientes</h2>

<table class="table table-striped">
    <tr>
        <th>Nome</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nome</td>
        </tr>
    }
</table>

